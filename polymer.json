{
  "lint": {
    "rules": [
      "polymer-2"
    ]
  },
  "builds": [{
    "name": "app",
    "js": {
      "minify": true,
      "compile": true
    },
    "css": {
      "minify": true
    },
    "html": {
      "minify": true
    }
  }, {
    "name": "src",
    "js": {
      "minify": false,
